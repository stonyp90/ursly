{
  "name": "ursly",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -n api,web -c cyan,magenta \"npm run start:api\" \"npm run start:web\"",
    "dev:all": "concurrently -n api,grpc,web -c cyan,yellow,magenta \"npm run start:api\" \"npm run start:grpc\" \"npm run start:web\"",
    "start": "nx serve",
    "start:api": "nx serve api",
    "start:grpc": "nx serve grpc",
    "start:web": "nx serve web",
    "start:agent": "nx serve agent-desktop",
    "start:vfs": "nx serve vfs-desktop",
    "build": "nx run-many --target=build --all",
    "build:api": "nx build api --configuration=production",
    "build:grpc": "nx build grpc --configuration=production",
    "build:web": "nx build web --configuration=production",
    "build:agent": "nx build agent-desktop",
    "build:vfs": "nx build vfs-desktop",
    "test": "nx run-many --target=test --all --passWithNoTests",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "e2e": "nx run-many --target=e2e --projects=web-e2e,api-e2e",
    "e2e:web": "nx e2e web-e2e",
    "e2e:web:ui": "nx e2e:ui web-e2e",
    "e2e:api": "nx e2e api-e2e",
    "e2e:report": "nx e2e:report web-e2e",
    "e2e:install": "npx playwright install",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "nx reset && rm -rf dist coverage node_modules/.cache",
    "prepare": "husky"
  },
  "private": true,
